# Git config from Quickstart
# qkst.io/dotfiles/gitconfig

[alias]
	who = shortlog -sn
	lg = log --pretty=oneline --all --topo-order
	po = push origin

[color]
	ui = true

[core]
	trustctime = false
	precomposeunicode = false
	autocrlf = input
	# editor = nano

[help]
	autocorrect = 1

[log]
	decorate = true
	abbrevCommit = true

[push]
	default = upstream
