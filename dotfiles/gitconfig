# Git config from Quickstart
# qkst.io/dotfiles/gitconfig

[user]
        # signingkey = YOURKEY

[alias]
	who = shortlog -sn
	lg = log --pretty=oneline --all --topo-order
	po = push origin
	fix = commit --fixup HEAD

[color]
	ui = true

[core]
	trustctime = false
	precomposeunicode = false
	autocrlf = input
	# editor = nano

[help]
	autocorrect = 1

[log]
	decorate = true
	abbrevCommit = true

[push]
	default = upstream

[rebase]
	autosquash = true
